<?xml version="1.0" encoding="utf-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
xmlns="http://java.sun.com/xml/ns/javaee"
xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" version="2.5">

	<servlet>
		<servlet-name>Logout</servlet-name>
		<servlet-class>net.tncy.servlet.Logout</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>Logout</servlet-name>
		<url-pattern>/Logout</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>CreateTravel</servlet-name>
		<servlet-class>net.tncy.servlet.CreateTravel</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>CreateTravel</servlet-name>
		<url-pattern>/CreateTravel</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>DisplayTravel</servlet-name>
		<servlet-class>net.tncy.servlet.DisplayTravel</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>DisplayTravel</servlet-name>
		<url-pattern>/DisplayTravel</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>DeleteTravel</servlet-name>
		<servlet-class>net.tncy.servlet.DeleteTravel</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>DeleteTravel</servlet-name>
		<url-pattern>/DeleteTravel</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>DisplayDetailsTravel</servlet-name>
		<servlet-class>net.tncy.servlet.DisplayDetailsTravel</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>DisplayDetailsTravel</servlet-name>
		<url-pattern>/DisplayDetailsTravel</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>BindTravel</servlet-name>
		<servlet-class>net.tncy.servlet.BindTravel</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>BindTravel</servlet-name>
		<url-pattern>/BindTravel</url-pattern>
	</servlet-mapping>
	
	<welcome-file-list>
		<welcome-file>index.jsp</welcome-file>
	</welcome-file-list>
</web-app>
